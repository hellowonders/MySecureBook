<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import com.jfoenix.controls.JFXButton?>

<StackPane prefWidth="231.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.qdt.blockchain.fx.controller.LicenseController">
  <children>
    <StackPane>
      <children>
        <VBox spacing="10.0">
          <children>
       		<Label fx:id="alertLabel" style="-fx-text-fill: red;"/>
            <GridPane>
              <children>
                <Label text="User ID :" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                <Label text="License :" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                <TextField fx:id="user" promptText="Use &quot;Email&quot;" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                <TextField fx:id="license" promptText="Use &quot;Serial key&quot;" GridPane.columnIndex="1" GridPane.rowIndex="1" />
              </children>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="109.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
            <StackPane prefHeight="-1.0" prefWidth="-1.0">
              <children>
                <JFXButton fx:id="licenseValidateButton" onAction="#licenseVerify" buttonType="RAISED" alignment="CENTER" text="Validate" StackPane.alignment="CENTER" style="-fx-background-color:WHITE;-fx-font-size:14px;" />
              </children>
            </StackPane>
          </children>
        </VBox>
      </children>
    </StackPane>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</StackPane>